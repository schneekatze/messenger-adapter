{
    "name": "enqueue/messenger-adapter",
    "type": "symfony-bundle",
    "description": "Enqueue adapter for Symfony Messenger component",
    "keywords": ["symfony", "messenger", "enqueue"],
    "homepage": "http://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Roze",
            "email": "samuel.roze@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        }
    ],
    "require": {
        "enqueue/enqueue-bundle": "^0.9.0",
        "symfony/messenger": "^4.1.0",
        "symfony/options-resolver": "^3.4|^4.1",
        "enqueue/amqp-tools": "^0.9.0"
    },
    "autoload": {
        "psr-4": { "Enqueue\\MessengerAdapter\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1@dev",
        "symfony/yaml": "^3.4@dev|^4.1@dev"
    }
}
